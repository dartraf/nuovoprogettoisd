Attribute VB_Name = "rptMedici"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit

Dim WithEvents sdfTipo  As StdDataFormat
Attribute sdfTipo.VB_VarHelpID = -1
Dim WithEvents sdfLunghezza As StdDataFormat
Attribute sdfLunghezza.VB_VarHelpID = -1

Private Sub DataReport_Initialize()
    Set sdfTipo = New StdDataFormat
    Set sdfLunghezza = New StdDataFormat
     
    With Me
        ' Centra il form che conterrà lanteprima
        .Left = (Screen.Width - .Width) / 2
        .Top = (Screen.Height - .Height) / 2
        With .Sections("level1").Controls
            .Item("txtCognome").DataField = "COGNOME"
            .Item("txtNome").DataField = "NOME"
            .Item("txtIndirizzo").DataField = "INDIRIZZO"
            .Item("txtCitta").DataField = "COMUNE"
            .Item("txtCap").DataField = "CAP"
            .Item("txtTelefono").DataField = "TELEFONO"
            .Item("txtCellulare").DataField = "CELLULARE"
            .Item("txtProvincia").DataField = "PROV"
            .Item("txtUfficio").DataField = "STUDIO"
            .Item("txtMail").DataField = "EMAIL"
            .Item("tctCodiceReg").DataField = "CODICE"
            Set .Item("txtCitta").DataFormat = sdfLunghezza
        End With
        With .Sections("corpo").Controls
            .Item("txtPaziente").DataMember = "RES1"
            .Item("txtPaziente").DataField = "PAZIENTE"
        End With
         With .Sections("pie").Controls
              .Item("lblDa").Caption = "da: " & tAccesso.cognome & " " & tAccesso.nome
         End With
    End With
End Sub

