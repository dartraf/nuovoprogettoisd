Attribute VB_Name = "rptPazienti"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit

Dim WithEvents sdfTipo  As StdDataFormat
Attribute sdfTipo.VB_VarHelpID = -1
Dim WithEvents sdfLunghezza As StdDataFormat
Attribute sdfLunghezza.VB_VarHelpID = -1

Private Sub DataReport_Initialize()
    Set sdfTipo = New StdDataFormat
    Set sdfLunghezza = New StdDataFormat
    With Me
    
        ' Centra il form che conterrà lanteprima
        .Left = (Screen.Width - .Width) / 2
        .Top = (Screen.Height - .Height) / 2
        With .Sections("corpo").Controls
            .Item("txtCognome").DataField = "COGNOME"
            .Item("txtNome").DataField = "PAZIENTINOME"
            .Item("txtDataNascita").DataField = "DATA_NASCITA"
            .Item("txtCodiceFiscale").DataField = "CODICE_FISCALE"
            .Item("txtIndirizzo").DataField = "INDIRIZZO"
            .Item("txtCitta").DataField = "COMUNINOME"
            .Item("txtCodice").DataField = "CODICE_DOCUMENTO"
            .Item("txtTelefono").DataField = "TELEFONO"
            .Item("txtCellulare").DataField = "CELLULARE"
            .Item("txtTipo").DataField = "TIPO_DOCUMENTO"
            
            If tFiltroStato.statoPaziente = TPOSPITE Then
                .Item("txtDataPrima").DataField = "DATA_A"
                .Item("txtDataSeconda").DataField = "DATA_P"
            ElseIf tFiltroStato.statoPaziente = tpDIALISI Then
                .Item("txtDataPrima").DataField = "DATA_INIZIO"
                .Item("txtDataSeconda").DataField = "DATA1"
            Else
                .Item("txtDataPrima").DataField = "STATODATA"
                .Item("txtDataSeconda").DataField = "DATA1"
            End If
            
            Set .Item("txtTipo").DataFormat = sdfTipo
            Set .Item("txtCitta").DataFormat = sdfLunghezza
        End With
        With .Sections("Pie").Controls
                .Item("lblDa").Caption = "da: " & tAccesso.cognome & " " & tAccesso.nome
        End With
    End With
End Sub

Private Sub sdfLunghezza_Format(ByVal DataValue As StdFormat.StdDataValue)
    If Len(Trim(DataValue)) > 12 Then
        DataValue = Left(Trim(DataValue), 12) & "..."
    End If
End Sub

Private Sub sdfTipo_Format(ByVal DataValue As StdFormat.StdDataValue)
    DataValue = Choose(DataValue + 1, "C.I.", "PTT", "PSP")
End Sub

